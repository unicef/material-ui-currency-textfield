    "core-util-is": "^1.0.2",
    "file-loader": "^4.1.0",
    "gh-pages": "^2.1.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "react-styleguidist": "^9.1.13",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.0.1",
